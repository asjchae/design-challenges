extends layout

block content
    .container-fluid
      .row-fluid(style="height: 120px;")
        span.span6
          h2.heading Team: #{teamname} 
          h4 Interests: #{interests}
        span.span6
          h4.heading Captain: #{captain.name}
          h5.heading Members: #{members}
          h5.heading Contact Email: #{captain.email}
      .row-fluid(style="height: 120px;")

        span.span6
          table.table
            thead
              tr
                th Open Projects
                th Go to
            tbody
              each proj, p in openprojects   

                tr
                  td #{proj.proj}   
                  td
                    input.btn.pull-center(type='button', value="View Project", onclick="location.href=\'" + proj.url+ "\';")View
                     
