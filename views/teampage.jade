              //-   td Text can go here
        span.span6
          table.table
            thead
              tr
                th Closed Projects
                th Go to 
            tbody
              each proj, p in closedprojects
                tr
                  td #{proj.proj}   
                  td
                    input.btn.pull-center(type='button', value="View Project", onclick="location.href=\'" + proj.url+ "\';")View
                     
                    
      .row-fluid
        span.span6
          table.table
            thead
              tr
                th Open Challenges
                th Go to 
            tbody
